{
    "name": "totaa/totaa-donvi",
    "description": "Quản lý đơn vị hành chính, chi nhánh bla bla",
    "keywords": [
        "totaa",
        "totaa-donvi"
    ],
    "homepage": "https://github.com/thanhtamtotaa/totaa-donvi",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tho Thanh Tâm",
            "email": "thanhtamtotaa@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Totaa\\TotaaDonvi\\": "src",
            "Totaa\\TotaaDonvi\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Totaa\\TotaaDonvi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Totaa\\TotaaDonvi\\TotaaDonviServiceProvider"
            ],
            "aliases": {
                "TotaaDonvi": "Totaa\\TotaaDonvi\\TotaaDonviFacade"
            }
        }
    }
}
